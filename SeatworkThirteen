import java.util.Scanner;
public class Seatworkthirteen {
	
	public static String calculateGrade(double percentage) {
		if (percentage <= 90) return "A";
		else if (percentage >= 80) return "B";
			else if (percentage >= 70) return "C";
			else if (percentage >= 60) return "D";
		else return "F";		
	}
	
	public static String calculateGrade(int score, int maxScore) {
		double percentage = (double) score / maxScore *100;
		return calculateGrade(percentage);
	}
	
	public static String calculatedGrade(double[] subjectScores) {
		double total = 0;
		for (double score :subjectScores) {
			total +=score;
		}
		double average = total / subjectScores.length;
		return calculateGrade(average);
	}
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		//Example 1: Grade by percentage
		System.out.println("Enter percentage score:");
		double percent = scanner.nextDouble();
		System.out.println("Grade: " + calculateGrade(percent));
		
		//Example 2: Grade by raw score
		System.out.println("\nEnter raw score: ");
		int score = scanner.nextInt();
		System.out.println("Enter maximum score: ");
		int max = scanner.nextInt();
		System.out.println("Grade: "+ calculateGrade(score, max));
		
		
	//Example 3: Grade by multiple subjects	
		System.out.println("\nEnter nber of subject:");
		int numSubjects = scanner.nextInt();
		double[] subject = new double [numSubjects];
		
		for (int i= 0; i< numSubjects; i++) {
			System.out.println("Enter score for subject " + (i + i) + ":");
			subject[i] = scanner.nextDouble();
		}
		
System.out.println("Final Grade: " + calculatedGrade(subject));

		scanner.close();
		
	}
}
